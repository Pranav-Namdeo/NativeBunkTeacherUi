{
  "name": "letsbunk-teacher",
  "version": "1.0.0",
  "description": "LetsBunk Teacher Attendance App",
  "main": "TeacherDashboard.js",
  "scripts": {
    "android": "react-native run-android",
    "start": "react-native start",
    "build:android": "cd android && gradlew assembleRelease",
    "build:debug": "cd android && gradlew assembleDebug",
    "clean": "cd android && gradlew clean",
    "install:apk": "adb install -r android/app/build/outputs/apk/release/app-release.apk",
    "logcat": "adb logcat *:E ReactNative:V"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.0",
    "@react-native-async-storage/async-storage": "^1.19.0",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/bottom-tabs": "^6.5.0",
    "react-native-vector-icons": "^10.0.0",
    "socket.io-client": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "metro-react-native-babel-preset": "^0.76.0"
  }
}
